{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/angular"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "build": {
      "env": {
        "NPM_FLAGS": "--legacy-peer-deps"
      }
    }
  }