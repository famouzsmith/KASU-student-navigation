{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/student-navigation/browser",
        "installCommand": "npm install --legacy-peer-deps",
        "buildCommand": "ng build --configuration production --base-href /"
      }
    }
  ],
  "routes": [
    {
      "src": "/_next/static/(.*)",
      "headers": { "Content-Type": "application/javascript" },
      "continue": true
    },
    {
      "src": "/(.*\\.js)",
      "headers": { "Content-Type": "application/javascript" },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}