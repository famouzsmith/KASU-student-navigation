{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/student-navigation/browser",
        "installCommand": "npm install --legacy-peer-deps",
        "buildCommand": "ng build --configuration production"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "maxDuration": 30,
    "memory": 3008
  }
}