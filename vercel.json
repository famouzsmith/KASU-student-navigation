{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/student-navigation/browser",
        "installCommand": "npm install --legacy-peer-deps",
        "buildCommand": "ng build --configuration production"
      }
    },
    {
      "src": "large-assets/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)", 
      "dest": "/index.html"
    }
  ]
}